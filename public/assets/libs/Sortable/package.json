    "prepublish": "./node_modules/grunt/bin/grunt"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rubaxa/Sortable.git"
  },
  "files": [
    "Sortable.js",
    "Sortable.min.js"
  ],
  "keywords": [
    "sortable",
    "reorder",
    "drag",
    "meteor",
    "angular",
    "ng-sortable",
